{
  "name": "react-organizational-chart",
  "version": "2.0.0",
  "description": "Simple react hierarchy tree - any React children accepted for nodes",
  "author": "daniel-hauser",
  "license": "MIT",
  "homepage": "https://daniel-hauser.github.io/react-organizational-chart",
  "repository": "daniel-hauser/react-organizational-chart",
  "main": "dist/index.js",
  "source": "src/index.tsx",
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "jest ./src",
    "test:watch": "jest ./src --watch",
    "build": "microbundle build --jsx React.createElement --external react",
    "start": "microbundle watch",
    "prepare": "npm run build",
    "prettier": "prettier --write 'src/**/*.ts[x]'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook --docs"
  },
  "dependencies": {
    "@emotion/core": "^10.1.1"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^25.1.1",
    "@types/react": "^16.14.8",
    "@types/react-dom": "^16.9.13",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "gh-pages": "^2.2.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-emotion": "^10.0.27",
    "microbundle": "^0.13.1",
    "prettier": "^2.3.0",
    "pretty-quick": "^2.0.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-is": "^16.13.1",
    "typescript": "^3.9.9"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "keywords": [
    "react",
    "organizational",
    "organizational-chart",
    "react-organizational-chart",
    "org-chart",
    "react-org-chart",
    "tree",
    "family",
    "family-tree"
  ],
  "files": [
    "dist"
  ]
}
